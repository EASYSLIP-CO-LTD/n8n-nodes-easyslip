{
  "name": "n8n-nodes-easyslip",
  "version": "1.0.0",
  "description": "n8n community node for EasySlip API - verify bank slips and Truemoney wallet slips",
  "keywords": [
    "n8n-community-node-package",
    "easyslip",
    "slip-verification",
    "bank-slip",
    "truemoney",
    "payment-verification"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": "EasySlip Developer",
    "email": "devops@racksync.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/EASYSLIP-CO-LTD/n8n-nodes-easyslip.git"
  },
  "engines": {
    "node": ">=20.15"
  },
  "main": "index.js",
  "scripts": {
    "build": "npx rimraf dist && tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lint:prepublish": "eslint -c .eslintrc.prepublish.js nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint:prepublish"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/EasySlipApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/EasySlip/EasySlip.node.js"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/parser": "~8.32.0",
    "eslint": "^8.57.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "gulp": "^5.0.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "form-data": "^4.0.3"
  }
}
